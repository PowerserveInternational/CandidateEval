@{
    ViewBag.Title = "Supervisees";
    List<Supervisee> supervisees = ViewBag.Supervisees;
}

<div class="jumbotron">
    <h1>Supervisees</h1>
</div>

<table>
    <thead>
        <tr>
            <th>Supervisee</th>
        </tr>
    </thead>
    <tbody>
        @{
            foreach (Supervisee supervisee in supervisees)
            {
                <tr>
                    <td>@supervisee.UserName</td>
                </tr>
            }
        }
    </tbody>
</table>